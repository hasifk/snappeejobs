{
	"name": "leejop/snappeejobs",
	"description": "The job portal",
	"keywords": ["job portal", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
        "laravelcollective/html": "5.1.*",
        "laravel/socialite" : "~2.0",
        "stripe/stripe-php": "~3.4.0",
        "twbs/bootstrap": "3.*",
        "twbs/bootstrap-sass": "3.*",
        "fortawesome/font-awesome": "4.*",
        "hieu-le/active": "~2.0",
		"doctrine/dbal": "~2.5",
	    "laracasts/utilities": "~2.1",
		"arcanedev/log-viewer": "3.7.0",
		"creativeorange/gravatar": "~1.0",
		"barryvdh/laravel-debugbar": "^2.0",
		"league/flysystem-aws-s3-v3": "~1.0",
		"intervention/image": "^2.3",
		"barryvdh/laravel-ide-helper": "^2.1",
		"laravel/cashier": "~5.0",
		"uxweb/sweet-alert": "^1.1",
		"aws/aws-sdk-php": "3.0",
		"predis/predis": "^1.0",
		"torann/geoip": "0.2.*@dev"


	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "phpspec/phpspec": "~2.1",
        "laracasts/generators": "~1.1",
        "potsky/laravel-localization-helpers" : "~1.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		},
        "files": [
            "app/helpers.php"
        ]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
          "php artisan clear-compiled",
          "php artisan ide-helper:generate",
          "php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
